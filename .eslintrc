{
  parser: "babel-eslint",
  "extends": "airbnb",
  settings: {
    'import/resolver': {
      webpack: {}
    }
  },
  "rules": {
    "react/jsx-filename-extension":"off",
    "import/extensions": [ 1, "never" ],
    "no-console": "off",
    "no-alert": "off",
    "quote-props": "off",
    "key-spacing": "off",
  }
}
